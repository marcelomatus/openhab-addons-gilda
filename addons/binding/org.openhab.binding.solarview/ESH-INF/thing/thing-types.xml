	<!-- Solarview energyProduction Binding -->
	<thing-type id="energyProduction">
		<supported-bridge-type-refs>
			<bridge-type-ref id="solarviewServer" />
		</supported-bridge-type-refs>

		<label>Solarview Energy Production Information</label>
		<description>Provides various energy data from the Solarview service</description>

		<channels>
			<channel id="KT0" typeId="KT0" />
			<channel id="KYR" typeId="KYR" />
			<channel id="KMT" typeId="KMT" />
			<channel id="KDY" typeId="KDY" />

			<channel id="PAC" typeId="PAC" />

			<channel id="UDC" typeId="UDC" />
			<channel id="IDC" typeId="IDC" />
			<channel id="UDCB" typeId="UDCB" />
			<channel id="IDCB" typeId="IDCB" />
			<channel id="UDCC" typeId="UDCC" />
			<channel id="IDCC" typeId="IDCC" />

			<channel id="UL1" typeId="UL1" />
			<channel id="IL1" typeId="IL1" />

			<channel id="TKK" typeId="TKK" />
		</channels>

		<config-description>
			<parameter name="refreshSecs" type="integer" min="1" step="1">
				<label>Refresh interval</label>
				<description>Interval that new time updates are posted to the event bus in seconds.</description>
				<required>false</required>
				<default>60</default>
				<advanced>true</advanced>
			</parameter>
		</config-description>
	</thing-type>

