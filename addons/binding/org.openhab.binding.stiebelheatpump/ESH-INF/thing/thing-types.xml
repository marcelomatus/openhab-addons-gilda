    <thing-type id="stiebelHeatPumpLWZ303_2_06">
        <label>Stiebel heat pump LWZ303 2.06</label>
        <description>Stiebel heat pump binding for LWZ/THZ connecting via serial RS232 interface to firmware version 2.06.</description>
	    <channel-groups>
	     <!-- at least 1 channel-group with id = version must be included  -->
            <channel-group id="version" typeId="channelGroupTypeVersion"/>
	        <channel-group id="time" typeId="channelGroupTypeTime"/>
	        <channel-group id="currentValues" typeId="channelGroupTypeCurrentValues"/>
            <channel-group id="operationCounters" typeId="channelGroupTypeOperationCounters"/>
            <channel-group id="nominalValues" typeId="channelGroupTypeNominalValues"/>

	    </channel-groups>
        <properties>
             <property name="vendor">Stiebel Eltron</property>
             <property name="modelId">LWZ303</property>
             <property name="protocol">Serial</property>
             <property name="firmware">2.06</property>
        </properties>
        <config-description>
            <parameter name="port" type="text" required="true">
                <label>Portname</label>
                <description>Serial port to use connecting to the heat pump device e.g. COM1 for Windows and /dev/ttyS0 or /dev/ttyUSB0 for Linux.</description>
                <default>/dev/ttyS4</default>
            </parameter>
            <parameter name="baudRate" type="decimal"  required="true">
                <label>Baud rate</label>
                <description>Baud rate of the serial interface.</description>
                <default>9600</default>
            </parameter>
            <parameter name="waitingTime" type="integer"  required="true">
                <label>Waiting time between requests</label>
                <description>Time between polling requests in [ms], it depends on the speed of the heat pump CPU and interface .</description>
                <default>1200</default>
            </parameter>
            <parameter name="refresh" type="decimal" required="true">
                <label>Heat pump refresh rate</label>
                <description>Refresh rate in [s] to query the meter data.</description>
                <default>60</default>
            </parameter>
        </config-description>
    </thing-type>
