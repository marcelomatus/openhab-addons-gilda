	<!-- DCDC -->
	<channel-group-type id="dcdc">
		<label>DC-DC Converter</label>
		<description>DC-DC Converter description</description>
		<channels>
			<channel id="PowerBattery" typeId="powerBattery"/>
			<channel id="VoltageBattery" typeId="voltageBattery"/>
			<channel id="EngergyBattery" typeId="engergyBattery"/>
			<channel id="PowerDCL" typeId="powerDCL"/>
			<channel id="VoltageDCL" typeId="voltageDCL"/>
			<channel id="EngergyDCL" typeId="engergyDCL"/>
			<channel id="FirmwareVersion" typeId="firmwareVersion"/>
			<channel id="FPGAVersion" typeId="FPGAVersion"/>
			<channel id="SerialNumber" typeId="serialNumber"/>
			<channel id="BoardVersion" typeId="boardVersion"/>
			<channel id="IsFlashing" typeId="isFlashing"/>

			<channel id="State" typeId="state"/>
			<channel id="StateText" typeId="stateText"/>
			<channel id="SubState" typeId="subState"/>
			<channel id="SubStateText" typeId="subStateText"/>
			<channel id="DeviceConnected" typeId="deviceConnected"/>
			<channel id="DeviceWorking" typeId="deviceWorking"/>
			<channel id="DeviceInService" typeId="deviceInService"/>
		</channels>
	</channel-group-type>

	<!-- PM -->
	<channel-group-type id="powermeter">
		<label>Power Meter</label>
		<description>Power Meter description</description>
		<channels>
			<channel id="PMType" typeId="PMType"/>
			<!-- 05: TAG_PM_REQ_DATA(TAG_PM_INDEX=0)/TAG_PM_REQ_ENERGY_L1 => TAG_PM_DATA/TAG_PM_ENERGY_L1 -->
			<channel id="PMCurrentEnergyL1" typeId="PMCurrentEngergy"/>
			<channel id="PMCurrentEnergyL2" typeId="PMCurrentEngergy"/>
			<channel id="PMCurrentEnergyL3" typeId="PMCurrentEngergy"/>
			<!-- 05: TAG_PM_REQ_DATA(TAG_PM_INDEX=0)/TAG_PM_REQ_POWER_L1 => TAG_PM_DATA/TAG_PM_POWER_L1 -->
			<channel id="PMCurrentPowerL1" typeId="PMCurrentPower"/>
			<channel id="PMCurrentPowerL2" typeId="PMCurrentPower"/>
			<channel id="PMCurrentPowerL3" typeId="PMCurrentPower"/>
			<!-- 05: TAG_PM_REQ_DATA(TAG_PM_INDEX=0)/TAG_PM_REQ_VOLTAGE_L1 => TAG_PM_DATA/TAG_PM_VOLTAGE_L1 -->
			<channel id="PMCurrentVoltageL1" typeId="PMCurrentVoltage"/>
			<channel id="PMCurrentVoltageL2" typeId="PMCurrentVoltage"/>
			<channel id="PMCurrentVoltageL3" typeId="PMCurrentVoltage"/>
			<!-- 05: TAG_PM_REQ_DATA(TAG_PM_INDEX=0)/TAG_PM_REQ_MODE => TAG_PM_DATA/TAG_PM_MODE -->
			<channel id="PMMode" typeId="PMMode"/>
			<!-- 05: TAG_PM_REQ_DATA(TAG_PM_INDEX=0)/TAG_PM_REQ_ACTIVE_PHASES => TAG_PM_DATA/TAG_PM_ACTIVE_PHASES -->
			<channel id="PMPhaseActiveL1" typeId="PMPhaseActive"/>
			<channel id="PMPhaseActiveL2" typeId="PMPhaseActive"/>
			<channel id="PMPhaseActiveL3" typeId="PMPhaseActive"/>
		</channels>
	</channel-group-type>

