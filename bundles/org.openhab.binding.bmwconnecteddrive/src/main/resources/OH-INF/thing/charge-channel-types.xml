	<channel-type id="profile-climate-channel">
		<item-type>Switch</item-type>
		<label>A/C at Departure Time</label>
	</channel-type>
	<channel-type id="profile-mode-channel">
		<item-type>String</item-type>
		<label>Charge Mode for Profile</label>
		<command>
			<options>
				<option value="IMMEDIATE_CHARGING">Immediate Charging</option>
				<option value="DELAYED_CHARGING">Prefer Charging in Charging Window</option>
			</options>
		</command>
	</channel-type>
	<channel-type id="profile-prefs-channel">
		<item-type>String</item-type>
		<label>Charging Preferences for Profile</label>
		<command>
			<options>
				<option value="NO_PRESELECTION">keine Vorauswahl</option>
				<option value="CHARGING_WINDOW">Ladezeitfenster</option>
			</options>
		</command>
	</channel-type>
	<channel-type id="window-start-channel">
		<item-type>DateTime</item-type>
		<label>Charge Window Start Time</label>
	</channel-type>
	<channel-type id="window-start-hour-channel">
		<item-type>Number</item-type>
		<label>Charge Window Start Hour</label>
		<state min="0" max="23" step="1" pattern="%d" readOnly="false"/>
	</channel-type>
	<channel-type id="window-start-minute-channel">
		<item-type>Number</item-type>
		<label>Charge Window Start Minute</label>
		<state min="0" max="55" step="5" pattern="%d" readOnly="false"/>
	</channel-type>
