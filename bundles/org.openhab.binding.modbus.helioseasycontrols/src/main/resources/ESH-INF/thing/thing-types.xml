<?xml version="1.0" encoding="UTF-8"?>
<thing:thing-descriptions bindingId="modbus"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:thing="https://openhab.org/schemas/thing-description/v1.0.0"
	xsi:schemaLocation="https://openhab.org/schemas/thing-description/v1.0.0 https://openhab.org/schemas/thing-description-1.0.0.xsd">

	<thing-type id="helios-ventilation-easycontrols">
	
		<supported-bridge-type-refs>
			<bridge-type-ref id="tcp" />
		</supported-bridge-type-refs>
	
		<label>Helios Ventilation</label>
		<description>Supports controlling a Helios Heat-Recovery Ventilation Device with easyControls using the Modbus Gateway TCP</description>
		<category>HVAC</category>
		
		<channel-groups>
			<channel-group id="general" typeId="general" />
			<channel-group id="operation" typeId="operation" />
			<channel-group id="unitConfig" typeId="unitConfig" />
			<channel-group id="profiles" typeId="profiles" />
			<channel-group id="humidityControl" typeId="humidityControl" />
			<channel-group id="co2Control" typeId="co2Control" />
			<channel-group id="vocControl" typeId="vocControl" />
		</channel-groups>
		
		<config-description>
			<parameter name="refreshInterval" type="integer">
				<label>Refresh Interval</label>
				<description>Binding's Refresh Interval in ms</description>
				<default>60000</default>
			</parameter>
		</config-description>
		
	</thing-type>

<!--  Channel Group Types -->
	
	<channel-group-type id="general">
		<label>General</label>
		<description>General Parameters / Status Information</description>
		<channels>
			<!--  General Status Infos -->
			<channel id="preHeaterStatus" typeId="preHeaterStatus"/>
			<channel id="bypassStatus" typeId="bypassStatus"/>
			<!--  Temperatures -->			
			<channel id="nhzDuctSensor" typeId="temperature"/>
			<channel id="nhzReturnSensor" typeId="temperature"/>
			<channel id="vhzDuctSensor" typeId="temperature"/>
			<channel id="tbd" typeId="tbd"/><!-- no description of this parameter available in the specification -->
			<channel id="temperatureOutsideAir" typeId="temperature"/>
			<channel id="temperatureSupplyAir" typeId="temperature"/>
			<channel id="temperatureOutgoingAir" typeId="temperature"/>
			<channel id="temperatureExtractAir" typeId="temperature"/>
			<!--  RPM -->
			<channel id="supplyAirRpm" typeId="rpm"/>
			<channel id="extractAirRpm" typeId="rpm"/>
			<!--  Filter Change -->
			<channel id="filterChangeRemainingTime" typeId="filterChangeRemainingTime"/>
			<channel id="filterChangeReset" typeId="filterChangeReset"/>
			<!--  Operating Hours -->
			<channel id="operatingHoursSupplyAirVent" typeId="operatingHours"/>
			<channel id="operatingHoursExtractAirVent" typeId="operatingHours"/>
			<channel id="operatingHoursVhz" typeId="operatingHours"/>
			<channel id="operatingHoursNhz" typeId="operatingHours"/>
			<!--  Power -->
			<channel id="outputPowerVhz" typeId="outputPower"/>
			<channel id="outputPowerNhz" typeId="outputPower"/>
			<!--  Infos / Warnings / Errors -->
			<channel id="errors" typeId="errors"/>
			<channel id="warnings" typeId="warnings"/>
			<channel id="infos" typeId="infos"/>
			<channel id="noOfErrors" typeId="noOfErrors"/>
			<channel id="noOfWarnings" typeId="noOfWarnings"/>
			<channel id="noOfInfos" typeId="noOfInfos"/>
			<channel id="errorsMsg" typeId="message"/>
			<channel id="warningsMsg" typeId="message"/>
			<channel id="infosMsg" typeId="message"/>
			<channel id="statusFlags" typeId="message"/>
			<channel id="clearError" typeId="clearError"/>
			<!--  Unit -->
			<channel id="articleDescription" typeId="articleDescription"/>
			<channel id="refNo" typeId="refNo"/>
			<channel id="macAddress" typeId="macAddress"/>
			<channel id="serNo" typeId="serNo"/>
			<channel id="prodCode" typeId="prodCode"/>
			<channel id="resetFlag" typeId="resetFlag"/>
			<channel id="factorySettingWzu" typeId="factorySettingWzu"/>
			<channel id="factoryReset" typeId="factoryReset"/>
			<!--  System Settings -->
			<channel id="language" typeId="language"/>
			<channel id="dateFormat" typeId="dateFormat"/>
			<channel id="date" typeId="date"/>
			<channel id="time" typeId="time"/>
			<channel id="timeZoneDifferenceToGmt" typeId="timeZoneDifferenceToGmt"/>
			<channel id="summerWinter" typeId="summerWinter"/>
			<!--  Software / Firmware -->
			<channel id="softwareVersionBasis" typeId="softwareVersionBasis"/>
			<channel id="autoSwUpdate" typeId="autoSwUpdate"/>
			<channel id="accessHeliosPortal" typeId="accessHeliosPortal"/>
			<channel id="globalManualWebUpdate" typeId="globalManualWebUpdate"/>
			<channel id="portalGlobalsErrorForWeb" typeId="portalGlobalsErrorForWeb"/>
			<channel id="logout" typeId="logout"/>
		</channels>
	</channel-group-type>
	
	<channel-group-type id="operation">
		<label>Operation</label>
		<description>Control of operating mode</description>
		<channels>
			<!--  Party Mode -->
			<channel id="partyModeDuration" typeId="partyModeDuration"/>
			<channel id="partyModeFanStage" typeId="partyModeFanStage"/>
			<channel id="partyModeRemainingTime" typeId="partyModeRemainingTime"/>
			<channel id="partyModeStatus" typeId="partyModeStatus"/>
			<!--  Standby Mode -->
			<channel id="standbyModeDuration" typeId="standbyModeDuration"/>
			<channel id="standbyModeFanStage" typeId="standbyModeFanStage"/>
			<channel id="standbyModeRemainingTime" typeId="standbyModeRemainingTime"/>
			<channel id="standbyModeStatus" typeId="standbyModeStatus"/>
			<!--  Holiday Programme -->
			<channel id="holidayProgramme" typeId="holidayProgramme"/>
			<channel id="holidayProgrammeFanStage" typeId="holidayProgrammeFanStage"/>
			<channel id="holidayProgrammeStart" typeId="holidayProgrammeStart"/>
			<channel id="holidayProgrammeEnd" typeId="holidayProgrammeEnd"/>
			<channel id="holidayProgrammeInterval" typeId="holidayProgrammeInterval"/>
			<channel id="holidayProgrammeActivationTime" typeId="holidayProgrammeActivationTime"/>
			<!--  Operating Mode / Fan Stage-->
			<channel id="operatingMode" typeId="operatingMode"/>
			<channel id="fanStage" typeId="fanStage"/>
			<channel id="percentageFanStage" typeId="percentageFanStage"/>
			<channel id="extractAirFanStage" typeId="fanStage"/>
			<channel id="supplyAirFanStage" typeId="fanStage"/>
		</channels>
	</channel-group-type>
	
	<channel-group-type id="unitConfig">
		<label>Unit Config</label>
		<description>Configuration parameters of the ventilation unit</description>
		<channels>
			<!--  Fan Stage Configuration -->
			<channel id="minFanStage" typeId="minFanStage"/>
			<channel id="assignmentFanStages" typeId="assignmentFanStages"/>
			<channel id="fanStageStepped0to2v" typeId="fanStageStepped1"/>
			<channel id="fanStageStepped2to4v" typeId="fanStageStepped2"/>
			<channel id="fanStageStepped4to6v" typeId="fanStageStepped2"/>
			<channel id="fanStageStepped6to8v" typeId="fanStageStepped2"/>
			<channel id="fanStageStepped8to10v" typeId="fanStageStepped2"/>
			<channel id="voltageFanStage1ExtractAir" typeId="voltageFanStage"/>
			<channel id="voltageFanStage2ExtractAir" typeId="voltageFanStage"/>
			<channel id="voltageFanStage3ExtractAir" typeId="voltageFanStage"/>
			<channel id="voltageFanStage4ExtractAir" typeId="voltageFanStage"/>
			<channel id="voltageFanStage1SupplyAir" typeId="voltageFanStage"/>
			<channel id="voltageFanStage2SupplyAir" typeId="voltageFanStage"/>
			<channel id="voltageFanStage3SupplyAir" typeId="voltageFanStage"/>
			<channel id="voltageFanStage4SupplyAir" typeId="voltageFanStage"/>
			<!--  Bypass Configuration -->
			<channel id="bypassRoomTemperature" typeId="bypassRoomTemperature"/>
			<channel id="bypassMinOutsideTemperature" typeId="bypassMinOutsideTemperature"/>
			<channel id="bypassFromDay" typeId="day"/>
			<channel id="bypassFromMonth" typeId="month"/>
			<channel id="bypassToDay" typeId="day"/>
			<channel id="bypassToMonth" typeId="month"/>
			<!--  Comfort Temperature -->
			<channel id="comfortTemp" typeId="comfortTemp"/>
			<!--  Error Output Function -->
			<channel id="errorOutputFunction" typeId="errorOutputFunction"/>
			<!--  Filter Change -->
			<channel id="filterChange" typeId="filterChange"/>
			<channel id="filterChangeInterval" typeId="filterChangeInterval"/>
			<!--  System Components and Extensions -->
			<channel id="unitConfig" typeId="unitConfig"/>
			<channel id="externalContact" typeId="externalContact"/>
			<channel id="functionTypeKwlEm" typeId="functionTypeKwlEm"/>
			<channel id="heatExchangerType" typeId="heatExchangerType"/>
			<channel id="kwlBe" typeId="kwlBe"/>
			<channel id="kwlBec" typeId="kwlBec"/>
			<channel id="vhzType" typeId="vhzType"/>
			<channel id="runOnTimeVhzNhz" typeId="runOnTimeVhzNhz"/>
			<channel id="offsetExtractAir" typeId="offsetExtractAir"/>
		</channels>
	</channel-group-type>
	
	<channel-group-type id="profiles">
		<label>Profiles</label>
		<description>Profiles for Device Operation</description>
		<channels>
			<channel id="weekProfileNhz" typeId="weekProfileNhz"/>
		</channels>
	</channel-group-type>
	
	<channel-group-type id="humidityControl">
		<label>Humidity Control</label>
		<description>Config of Sensors for Humidity and/or Temperature Control</description>
		<channels>
			<channel id="humidityControlStatus" typeId="humidityControlStatus"/>
			<channel id="humidityControlSetValue" typeId="humidityControlSetValue"/>
			<channel id="humidityControlSteps" typeId="humidityControlSteps"/>
			<channel id="humidityStopTime" typeId="humidityStopTime"/>
			<!--  Configuration Temperature+Humidity / Only Humidity / Only Temperature -->
			<channel id="kwlFtfConfig0" typeId="kwlFtfConfig"/>
			<channel id="kwlFtfConfig1" typeId="kwlFtfConfig"/>
			<channel id="kwlFtfConfig2" typeId="kwlFtfConfig"/>
			<channel id="kwlFtfConfig3" typeId="kwlFtfConfig"/>
			<channel id="kwlFtfConfig4" typeId="kwlFtfConfig"/>
			<channel id="kwlFtfConfig5" typeId="kwlFtfConfig"/>
			<channel id="kwlFtfConfig6" typeId="kwlFtfConfig"/>
			<channel id="kwlFtfConfig7" typeId="kwlFtfConfig"/>
			<!--  Sensors Active -->
			<channel id="sensorConfigKwlFtf1" typeId="sensorConfig"/>
			<channel id="sensorConfigKwlFtf2" typeId="sensorConfig"/>
			<channel id="sensorConfigKwlFtf3" typeId="sensorConfig"/>
			<channel id="sensorConfigKwlFtf4" typeId="sensorConfig"/>
			<channel id="sensorConfigKwlFtf5" typeId="sensorConfig"/>
			<channel id="sensorConfigKwlFtf6" typeId="sensorConfig"/>
			<channel id="sensorConfigKwlFtf7" typeId="sensorConfig"/>
			<channel id="sensorConfigKwlFtf8" typeId="sensorConfig"/>
			<!--  Sensor Names -->
			<channel id="sensorNameHumidityAndTemp1" typeId="sensorName"/>
			<channel id="sensorNameHumidityAndTemp2" typeId="sensorName"/>
			<channel id="sensorNameHumidityAndTemp3" typeId="sensorName"/>
			<channel id="sensorNameHumidityAndTemp4" typeId="sensorName"/>
			<channel id="sensorNameHumidityAndTemp5" typeId="sensorName"/>
			<channel id="sensorNameHumidityAndTemp6" typeId="sensorName"/>
			<channel id="sensorNameHumidityAndTemp7" typeId="sensorName"/>
			<channel id="sensorNameHumidityAndTemp8" typeId="sensorName"/>
			<!--  External Humidity Sensors -->
			<channel id="externalSensorKwlFtfHumidity1" typeId="humidity"/>
			<channel id="externalSensorKwlFtfHumidity2" typeId="humidity"/>
			<channel id="externalSensorKwlFtfHumidity3" typeId="humidity"/>
			<channel id="externalSensorKwlFtfHumidity4" typeId="humidity"/>
			<channel id="externalSensorKwlFtfHumidity5" typeId="humidity"/>
			<channel id="externalSensorKwlFtfHumidity6" typeId="humidity"/>
			<channel id="externalSensorKwlFtfHumidity7" typeId="humidity"/>
			<channel id="externalSensorKwlFtfHumidity8" typeId="humidity"/>
			<!--  External Temperature Sensors -->
			<channel id="externalSensorKwlFtfTemperature1" typeId="temperature"/>
			<channel id="externalSensorKwlFtfTemperature2" typeId="temperature"/>
			<channel id="externalSensorKwlFtfTemperature3" typeId="temperature"/>
			<channel id="externalSensorKwlFtfTemperature4" typeId="temperature"/>
			<channel id="externalSensorKwlFtfTemperature5" typeId="temperature"/>
			<channel id="externalSensorKwlFtfTemperature6" typeId="temperature"/>
			<channel id="externalSensorKwlFtfTemperature7" typeId="temperature"/>
			<channel id="externalSensorKwlFtfTemperature8" typeId="temperature"/>
		</channels>
	</channel-group-type>
	
	<channel-group-type id="co2Control">
		<label>CO2 Control</label>
		<description>Config of Sensors for CO2 Control</description>
		<channels>
			<channel id="co2ControlStatus" typeId="co2ControlStatus"/>
			<channel id="co2ControlSetValue" typeId="co2ControlSetValue"/>
			<channel id="co2ControlSteps" typeId="co2ControlSteps"/>
			<!--  Sensor Names -->
			<channel id="sensorNameCo21" typeId="sensorName"/>
			<channel id="sensorNameCo22" typeId="sensorName"/>
			<channel id="sensorNameCo23" typeId="sensorName"/>
			<channel id="sensorNameCo24" typeId="sensorName"/>
			<channel id="sensorNameCo25" typeId="sensorName"/>
			<channel id="sensorNameCo26" typeId="sensorName"/>
			<channel id="sensorNameCo27" typeId="sensorName"/>
			<channel id="sensorNameCo28" typeId="sensorName"/>
			<!--  External CO2 Sensors -->
			<channel id="externalSensorKwlCo21" typeId="co2"/>
			<channel id="externalSensorKwlCo22" typeId="co2"/>
			<channel id="externalSensorKwlCo23" typeId="co2"/>
			<channel id="externalSensorKwlCo24" typeId="co2"/>
			<channel id="externalSensorKwlCo25" typeId="co2"/>
			<channel id="externalSensorKwlCo26" typeId="co2"/>
			<channel id="externalSensorKwlCo27" typeId="co2"/>
			<channel id="externalSensorKwlCo28" typeId="co2"/>
		</channels>
	</channel-group-type>
	
	<channel-group-type id="vocControl">
		<label>VOC Control</label>
		<description>Config of Sensors for VOC Control</description>
		<channels>
			<channel id="vocControlStatus" typeId="vocControlStatus"/>
			<channel id="vocControlSetValue" typeId="vocControlSetValue"/>
			<channel id="vocControlSteps" typeId="vocControlSteps"/>
			<!--  Sensor Names -->
			<channel id="sensorNameVoc1" typeId="sensorName"/>
			<channel id="sensorNameVoc2" typeId="sensorName"/>
			<channel id="sensorNameVoc3" typeId="sensorName"/>
			<channel id="sensorNameVoc4" typeId="sensorName"/>
			<channel id="sensorNameVoc5" typeId="sensorName"/>
			<channel id="sensorNameVoc6" typeId="sensorName"/>
			<channel id="sensorNameVoc7" typeId="sensorName"/>
			<channel id="sensorNameVoc8" typeId="sensorName"/>
			<!--  External CO2 Sensors -->
			<channel id="externalSensorKwlVoc1" typeId="voc"/>
			<channel id="externalSensorKwlVoc2" typeId="voc"/>
			<channel id="externalSensorKwlVoc3" typeId="voc"/>
			<channel id="externalSensorKwlVoc4" typeId="voc"/>
			<channel id="externalSensorKwlVoc5" typeId="voc"/>
			<channel id="externalSensorKwlVoc6" typeId="voc"/>
			<channel id="externalSensorKwlVoc7" typeId="voc"/>
			<channel id="externalSensorKwlVoc8" typeId="voc"/>
		</channels>
	</channel-group-type>


<!--  Channel Types -->

	<channel-type id="articleDescription" advanced="true">
		<item-type>String</item-type>
		<label>Article Description</label>
		<description>The KWL's article description</description>
	</channel-type>
	
	<channel-type id="refNo" advanced="true">
		<item-type>String</item-type>
		<label>Reference Number</label>
		<description>The KWL's reference number</description>
	</channel-type>
	
	<channel-type id="macAddress" advanced="true">
		<item-type>String</item-type>
		<label>MAC Address</label>
		<description>The KWL's MAC Address</description>
		<state readOnly="true" />
	</channel-type>
	
	<channel-type id="language" advanced="true">
		<item-type>String</item-type>
		<label>Language</label>
		<description>The KWL user interface's language</description>
		<state>
			<options>
				<option value="de">Deutsch</option>
				<option value="en">English</option>
				<option value="fr">Français</option>
				<option value="hr">Hrvatski</option>
				<option value="hu">Magyar</option>
				<option value="it">Italiano</option>
				<option value="pl">Polski</option>
				<option value="sk">Slovenský</option>
				<option value="sl">Slovenščina</option>
			</options>
		</state>
	</channel-type>
	
	<channel-type id="date" advanced="true">
		<item-type>DateTime</item-type>
		<label>Date</label>
		<description>The KWL's system date</description>
		<category>Time</category>
	</channel-type>
	
	<channel-type id="time" advanced="true">
		<item-type>DateTime</item-type>
		<label>Time</label>
		<description>The KWL's system time</description>
		<category>Time</category>
	</channel-type>
	
	<channel-type id="summerWinter" advanced="true">
		<item-type>Switch</item-type>
		<label>Summertime / Wintertime</label>
		<description>Indicates if summertime or wintertime is active</description>
		<category>Time</category>
		<state>
			<options>
				<option value="0">Wintertime</option>
				<option value="1">Summertime</option>
			</options>
		</state>
	</channel-type>
	
	<channel-type id="autoSwUpdate" advanced="true">
		<item-type>Switch</item-type>
		<label>Automatic SW Updates</label>
		<description>Indicates if automatic software updates are enabled</description>
		<state>
			<options>
				<option value="0">disabled</option>
				<option value="1">enabled</option>
			</options>
		</state>
	</channel-type>
	
	<channel-type id="accessHeliosPortal" advanced="true">
		<item-type>Switch</item-type>
		<label>Access Helios Portal</label>
		<description>Indicates if access to Helios portal is enabled</description>
		<state>
			<options>
				<option value="0">disabled</option>
				<option value="1">enabled</option>
			</options>
		</state>
	</channel-type>
	
	<channel-type id="voltageFanStage" advanced="true">
		<item-type>Number</item-type>
		<label>Voltage for Fan Stages</label>
		<description>Voltage for supply or extract air fan mapped to fan stage (1-4)</description>
		<category>HVAC</category>
		<state min="1.6" max="10" step="0.1" pattern="%.1f %unit%" readOnly="false"/>
	</channel-type>
	
	<channel-type id="minFanStage" advanced="true">
		<item-type>Number</item-type>
		<label>Minimum Fan Stage</label>
		<description>Minimum fan stage (0 or 1)</description>
		<category>HVAC</category>
		<state min="0" max="1" step="1" readOnly="false"/>
	</channel-type>
	
	<channel-type id="kwlBe" advanced="true">
		<item-type>Switch</item-type>
		<label>KWL-BE Activated</label>
		<description>Slide switch controller KWL-BE activated</description>
		<state>
			<options>
				<option value="0">Off</option>
				<option value="1">On</option>
			</options>
		</state>
	</channel-type>
	
	<channel-type id="kwlBec" advanced="true">
		<item-type>Switch</item-type>
		<label>KWL-BEC Activated</label>
		<description>Comfort controller KWL-BEC activated</description>
		<state>
			<options>
				<option value="0">Off</option>
				<option value="1">On</option>
			</options>
		</state>
	</channel-type>
	
	<channel-type id="unitConfig" advanced="true">
		<item-type>Number</item-type>
		<label>Ventilation Unit Configuration</label>
		<description>Ventilation unit configuration (type of house)</description>
		<state>
			<options>
				<option value="1">DiBt</option>
				<option value="2">Passive-house</option>
			</options>
		</state>
	</channel-type>
	
	<channel-type id="preHeaterStatus" advanced="true">
		<item-type>Switch</item-type>
		<label>Pre-Heater Status</label>
		<description>Pre-Heater Status</description>
		<state>
			<options>
				<option value="0">Off</option>
				<option value="1">On</option>
			</options>
		</state>
	</channel-type>
	
	<channel-type id="kwlFtfConfig" advanced="true">
		<item-type>Number</item-type>
		<label>Humidity/Temperature Sensor Configuration</label>
		<description>Humidity/temperature sensor configuration</description>
		<state>
			<options>
				<option value="1">Only humidity</option>
				<option value="2">Only temperature</option>
				<option value="3">Combined</option>
			</options>
		</state>
	</channel-type>
	
	<channel-type id="humidityControlStatus" advanced="true">
		<item-type>Number</item-type>
		<label>Humidity Control Status</label>
		<description>Humidity control status</description>
		<category>Humidity</category>
		<state>
			<options>
				<option value="0">Off</option>
				<option value="1">Stepped</option>
				<option value="2">Stepless</option>
			</options>
		</state>
	</channel-type>
	
	<channel-type id="humidityControlSetValue" advanced="true">
		<item-type>Number</item-type>
		<label>Humidity Control Set Value</label>
		<description>Humidity control set value (in percent)</description>
		<category>Humidity</category>
		<state min="20" max="80" step="1" pattern="%d %unit%" readOnly="false"/>
	</channel-type>
	
	<channel-type id="humidityControlSteps" advanced="true">
		<item-type>Number</item-type>
		<label>Humidity Control Steps</label>
		<description>Humidity control steps (in percent)</description>
		<category>Humidity</category>
		<state min="5" max="20" step="1" pattern="%d %unit%" readOnly="false"/>
	</channel-type>
	
	<channel-type id="humidityStopTime" advanced="true">
		<item-type>Number</item-type>
		<label>Humidity Stop Time</label>
		<description>Humidity stop time in hours (0-24)</description>
		<category>Humidity</category>
		<state min="0" max="24" step="1" pattern="%d %unit%" readOnly="false"/>
	</channel-type>
	
	<channel-type id="co2ControlStatus" advanced="true">
		<item-type>Number</item-type>
		<label>CO2 Control Status</label>
		<description>CO2 control status</description>
		<category>CarbonDioxide</category>
		<state>
			<options>
				<option value="0">Off</option>
				<option value="1">Stepped</option>
				<option value="2">Stepless</option>
			</options>
		</state>
	</channel-type>
	
	<channel-type id="co2ControlSetValue" advanced="true">
		<item-type>Number</item-type>
		<label>CO2 Control Set Value</label>
		<description>CO2 control set value (in ppm)</description>
		<category>CarbonDioxide</category>
		<state min="300" max="2000" step="1" pattern="%d %unit%" readOnly="false"/>
	</channel-type>
	
	<channel-type id="co2ControlSteps" advanced="true">
		<item-type>Number</item-type>
		<label>CO2 Control Steps</label>
		<description>CO2 control steps (in ppm)</description>
		<category>CarbonDioxide</category>
		<state min="50" max="400" step="1" pattern="%d %unit%" readOnly="false"/>
	</channel-type>
	
