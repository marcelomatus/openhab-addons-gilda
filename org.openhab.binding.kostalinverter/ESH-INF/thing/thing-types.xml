        <channel typeId="dxsEntries_22" id="dxsEntries_22" ></channel>
        </channels>

        <config-description>
            <parameter name="url" type="text" required="true">
                <label>URL</label>
                <description>URL of the web interface (e.g. http://192.168.0.1)</description>
                <default></default>
            </parameter>
            
            <parameter name="userName" type="text" required="true">
                <label>Username</label>
                <default>pvserver</default>
            </parameter>
            
            <parameter name="password" type="text" required="true">
                <label>Password</label>
                <default>pvwr</default>
            </parameter>

            <parameter name="refreshInterval" type="integer">
                <label>Refresh Interval</label>
                <description>States how often a refresh shall occur (in ms)</description>
                <default>60</default>
            </parameter>
            
            <parameter name="type" type="text" required="true">
                <label>Type of Inverter</label>
                <description>Depends on type of inverter</description>
                <default>new_generation</default>
            </parameter> 
            
            <parameter name="dxsEntries_ChannelCfgFile" type="text">
                <label>dxsEntries_/Channel Configuration file</label>
                <description>Path and name of file</description>
                <default>60</default>
            </parameter>   
                
        </config-description>

