{
  "problemMatcher": [
    {
      "owner": "openhab-compile-problems",
      "severity": "error",
      "pattern": [
        {
          "regexp": "Failed to execute goal.*Compilation failure"
        },
        {
          "regexp": "^\\[ERROR\\] (.+\\.java):\\[(.+),(.+)\\] (.*)$",
          "file": 1,
          "line": 2,
          "col": 3,
          "message": 4,
          "loop": true
        }
      ]
    },
    {
      "owner": "openhab-compile-warnings",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^\\[WARNING\\] (.+\\.java):\\[(.+),(.+)\\] (.*)$",
          "file": 1,
          "line": 2,
          "col": 3,
          "message": 4
        }
      ]
    }
  ]
}
